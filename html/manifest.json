{
  "manifest_version": 2,
  "name": "Chihiro",
  "version": "1",
  
  "permissions": [
    "activeTab",
    "tabs"
  ],
  
  "icons": {"128": "dart_icon.png"},
  
  "background": {
    "persistent": false,
    "page": "eventpage.html"
  },
  
  "browser_action": {
    "default_icon": "dart_icon.png"
  },
  
  "web_accessible_resources": [
    "data/JMdict.txt",
    "tabpage.js",
    "klstyle.css"
  ] 
}
